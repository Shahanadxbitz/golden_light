 "font": "Default",
 "html": "{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n\n<table class=\"table table-outlined\">\n    <thead>\n        <tr>\n            <td colspan=\"6\">\n                <table class=\"table-doc\">\n                    <tbody>\n                        <tr>\n                            <td>\n                                <table class=\"table-doc\">\n                                    <tbody>\n                                        <tr>\n                                            <td style=\"width: 100px;\">{{ _('Customer') }}</td>\n                                            <td>:</td>\n                                            <td><strong>{{ doc.customer_name }}</strong></td>\n                                        </tr>\n                                    {% if doc.cash_customer_name %}\n                                        <tr>\n                                            <td>{{ _('Cash Customer') }}</td>\n                                            <td>:</td>\n                                            <td><strong>{{ doc.cash_customer_name }}</strong></td>\n                                        </tr>\n                                    {% endif %}\n                                    </tbody>\n                                </table>\n                            </td>\n                            <td>\n                                <table class=\"table-doc\">\n                                    <tbody>\n                                        <tr>\n                                            <td>{{ _('Invoice No.') }}</td>\n                                            <td>:</td>\n                                            <td style=\"width: 140px\"><strong>{{ doc.name }}</strong></td>\n                                        </tr>\n                                        <tr>\n                                            <td>{{ _('Date') }}</td>\n                                            <td>:</td>\n                                            <td><strong>{{ frappe.format(doc.posting_date or doc.transaction_date, { 'fieldtype': 'Date' }) }}</strong></td>\n                                        </tr>\n                                        <tr>\n                                            <td>{{ _('Time') }}</td>\n                                            <td>:</td>\n                                            <td><strong>{{ frappe.utils.format_time(doc.posting_time or doc.creation, \"hh:mm a\") }}</strong></td>\n                                        </tr>\n                                        {% if doc.po_no %}\n                                            <tr>\n                                                <td>{{ _('P.O No.') }}</td>\n                                                <td>:</td>\n                                                <td><strong>{{ doc.po_no }}</strong></td>\n                                            </tr>\n                                        {% endif %}\n                                        {% if doc.po_date %}\n                                            <tr>\n                                                <td>{{ _('P.O Date') }}</td>\n                                                <td>:</td>\n                                                <td><strong>{{ frappe.format(doc.po_date, { 'fieldtype': 'Date' }) }}</strong></td>\n                                            </tr>\n                                        {% endif %}\n                                    </tbody>\n                                </table>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </td>\n        </tr>\n        <tr>\n            <th style=\"width: 3%;\" class=\"text-left\">#</th>\n            <th style=\"width: 35%;\" class=\"text-left\">{{ _('Item') }}</th>\n            <th style=\"width: 10%;\" class=\"text-right\">{{ _('Carton') }}</th>\n            <th style=\"width: 10%;\" class=\"text-right\">{{ _('Qty') }}</th>\n            <th style=\"width: 13%;\" class=\"text-right\">{{ _('Rate') }}</th>\n            <th style=\"width: 16%;\" class=\"text-right\">{{ _('Amount') }}</th>\n        </tr>\n    </thead>\n    <tbody>\n        {%- set total_cartons = namespace(value=0) -%}\n        {%- for row in get_items(doc.doctype, doc.name).values() -%}\n    \t\t<tr>\n    \t\t\t<td class=\"text-right\">{{ row.idx }}</td>\n    \t\t\t<td class=\"text-left\"><div style=\"border: 0px;\">{{ row.description }}</div></td>\n    \t\t\t<td class=\"text-right\">\n    \t\t\t    {%- set carton_rate = frappe.db.get_value(\"UOM Conversion Detail\", { \"parenttype\": \"Item\", \"parent\": row.item_code, \"uom\": \"Carton\"}, \"conversion_factor\") or 0 -%}\n    \t\t\t    {% if carton_rate > 0 %}\n        \t\t\t    {%- set cartons = row.qty / carton_rate %}\n        \t\t\t    {%- set total_cartons.value = total_cartons.value + cartons -%}\n        \t\t\t    {{ get_int(cartons) }}\n    \t\t\t    {% endif %}\n    \t\t\t</td>\n    \t\t\t<td class=\"text-right\">{{ get_int(row.qty) }}</td>\n    \t\t\t<td class=\"text-right\">{{ frappe.utils.fmt_money(row.rate) }}</td>\n    \t\t\t<td class=\"text-right\">{{ frappe.utils.fmt_money(row.rate * row.qty) }}</td>\n    \t\t</tr>\n\t    {%- endfor -%}\n    </tbody>\n    <tfoot class=\"text-right\">\n        <tr>\n            <td colspan=\"2\"></td>\n            <td><strong>{{ total_cartons.value }}</strong></td>\n            <td><strong>{{ get_int(doc.total_qty) }}</strong></td>\n            <td>{{ _('Total') }}</td>\n            <td><strong>{{ doc.get_formatted('total') }}</strong></td>\n        </tr>\n        \n        {% set rowspan = 1 %}\n        {% if doc.previous_outstanding != 0 %}\n            {% set rowspan = rowspan + 1 %}\n        {% endif %}\n        {% if doc.current_outstanding != 0 %}\n            {% set rowspan = rowspan + 1 %}\n        {% endif %}\n        \n        <tr>\n            <td colspan=\"4\" rowspan=\"{{ rowspan }}\" class=\"text-left\">\n                {{ _('Amount in Words:') }}<br>\n                <strong>{{ _(doc.in_words) }}</strong>\n            </td>\n            <td>{{ _('Grand Total') }}</td>\n            <td><strong>{{ doc.get_formatted('grand_total') }}</strong></td>\n        </tr>\n\n        {% if doc.customer != \"\u0646\u0642\u062f\u064a \u0628\u064a\u0639 \u0648\u0634\u0631\u0627\u0621\" %}\n            {% if doc.previous_outstanding != 0 %}\n                <tr>\n                    <td>{{ _(\"Previous Outstanding\") }}</td>\n                    <td><strong>{{ doc.get_formatted('previous_outstanding') }}</strong></td>\n                </tr>\n            {% endif %}\n            {% if doc.current_outstanding != 0 %}\n                <tr>\n                    <td>{{ _(\"Current Outstanding\") }}</td>\n                    <td><strong>{{ doc.get_formatted('current_outstanding') }}</strong></td>\n                </tr>\n            {% endif %}\n        {% endif %}\n    </tfoot>\n</table>\n\n{% if doc.terms and (doc.terms|striptags|trim): %}\n    <div>\n        <h4>{{ _('Terms and Conditions') }}</h4>\n        <div style=\"font-size: 0.875em; color: #333;\">{{ doc.terms }}</div>\n    </div>\n{% endif %}\n\n{% if footer %}\n    {{ footer }}\n{% endif %}\n",
