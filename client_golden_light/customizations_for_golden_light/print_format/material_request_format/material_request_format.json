 "html": "<table class=\"table table-bordered table-items\">\n    <thead>\n        <tr>\n            <td colspan=\"6\">\n                <table class=\"doc-info\">\n                    <tr>\n                        \n                        <td class=\"info\">\n                           <table class=\"table\">\n                                <tr>\n                                    <td>{{ _('Request No.') }}</td>\n                                    <td>:</td>\n                                    <td><strong>{{ doc.name }}</strong></td>\n                                </tr>\n                                <tr>\n                                    <td>{{ _('Date') }}</td>\n                                    <td>:</td>\n                                    <td><strong>{{ frappe.format(doc.posting_date or doc.transaction_date, { 'fieldtype': 'Date' }) }}</strong></td>\n                                </tr>\n                            </table> \n                        </td>\n                        <td class=\"party\">\n                            \n                        </td>\n                    </tr>\n                </table>\n            </td>\n        </tr>\n        <tr>\n            <th class=\"text-left\">#</th>\n            <th class=\"text-left\">{{ _(\"Source Warehouse\") }}</th>\n            <th class=\"text-left\">{{ _(\"Target Warehouse\") }}</th>\n            <th class=\"text-left\">{{ _('Item') }}</th>\n\t\t\t<th class=\"text-right\">{{ _('Carton') }}</th>\n            <th class=\"text-right\">{{ _('Qty') }}</th>\n        </tr>\n    </thead>\n    <tbody>\n        {%- set total_cartons = namespace(value=0) -%}\n        {%- for row in doc.items -%}\n    \t\t<tr>\n    \t\t\t<td class=\"text-right\">{{ row.idx }}</td>\n    \t\t\t<td>{{ row.from_warehouse }}</td>\n    \t\t\t<td>{{ row.warehouse }}</td>\n    \t\t\t<td class=\"text-left\">{{ row.item_name }}</td>\n    \t\t\t<td class=\"text-right\">\n    \t\t\t    {%- set carton_rate = frappe.db.get_value(\"UOM Conversion Detail\", { \"parenttype\": \"Item\", \"parent\": row.item_code, \"uom\": \"Carton\"}, \"conversion_factor\") or 0 -%}\n    \t\t\t    {% if carton_rate > 0 %}\n        \t\t\t    {%- set cartons = row.qty / carton_rate %}\n        \t\t\t    {%- set total_cartons.value = total_cartons.value + cartons -%}\n        \t\t\t    {{ get_int(cartons) }}\n    \t\t\t    {% endif %}\n    \t\t\t</td>\n    \t\t\t<td class=\"text-right\">{{ get_int(row.qty) }} {{ _(row.uom or row.stock_uom) }}</td>\n    \t\t</tr>\n\t    {%- endfor -%}\n    </tbody>\n</table>\n\n{% if doc.terms and (doc.terms|striptags|trim): %}\n    <div>\n        <h4>{{ _('Terms and Conditions') }}</h4>\n        <div style=\"font-size: 0.875em; color: #333;\">{{ doc.terms }}</div>\n    </div>\n{% endif %}",
