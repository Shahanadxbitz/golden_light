 "doctype": "Print Format",
 "font": "Default",
 "html": "{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n\n<table class=\"table table-bordered table-items\">\n    <thead>\n        <tr>\n            <td colspan=\"5\">\n                <table class=\"doc-info\">\n                    <tr>\n                        <td class=\"party\">\n                            <p>{{ _('Order To:') }}</p>\n                            <h4><strong>{{ doc.customer_name }}</strong></h4>\n                            <p>{{ doc.address_display or \"\" }}</p>\n                        </td>\n                        <td class=\"info\">\n                           <table class=\"table\">\n                                <tr>\n                                    <td>{{ _('Order No.') }}</td>\n                                    <td>:</td>\n                                    <td><strong>{{ doc.name }}</strong></td>\n                                </tr>\n                                <tr>\n                                    <td>{{ _('Date') }}</td>\n                                    <td>:</td>\n                                    <td><strong>{{ frappe.format(doc.posting_date or doc.transaction_date, { 'fieldtype': 'Date' }) }}</strong></td>\n                                </tr>\n                                {% if doc.po_no %}\n                                    <tr>\n                                        <td>{{ _('P.O No.') }}</td>\n                                        <td>:</td>\n                                        <td><strong>{{ doc.po_no }}</strong></td>\n                                    </tr>\n                                {% endif %}\n                                {% if doc.po_date %}\n                                    <tr>\n                                        <td>{{ _('P.O Date') }}</td>\n                                        <td>:</td>\n                                        <td><strong>{{ frappe.format(doc.po_date, { 'fieldtype': 'Date' }) }}</strong></td>\n                                    </tr>\n                                {% endif %}\n                            </table> \n                        </td>\n                    </tr>\n                </table>\n            </td>\n        </tr>\n        <tr>\n            <th class=\"text-left\">#</th>\n            <th class=\"text-left\">{{ _('Item') }}</th>\n            <th class=\"text-right\">{{ _('Qty') }}</th>\n            <th class=\"text-right\">{{ _('Rate') }}</th>\n            <th class=\"text-right\">{{ _('Amount') }}</th>\n        </tr>\n    </thead>\n    <tbody>\n        {%- for row in doc.items -%}\n    \t\t<tr>\n    \t\t\t<td class=\"text-right\" style=\"width: 3%;\">{{ row.idx }}</td>\n    \t\t\t<td class=\"text-left\" style=\"width: 37%;\"><div style=\"border: 0px;\">{{ row.description }}</div></td>\n    \t\t\t<td class=\"text-right\" style=\"width: 10%;\">{{ get_int(row.qty) }} {{ row.uom or row.stock_uom }}</td>\n    \t\t\t<td class=\"text-right\" style=\"width: 15%;\">{{ row.get_formatted(\"rate\", doc) }}</td>\n    \t\t\t<td class=\"text-right\" style=\"width: 15%;\">{{ row.get_formatted(\"amount\", doc) }}</td>\n    \t\t</tr>\n\t    {%- endfor -%}\n    </tbody>\n    <tfoot class=\"text-right font-weight-bold\">\n        <tr>\n            <td colspan=\"3\" rowspan=\"4\" class=\"text-left\">\n                <p>{{ _('Amount in Words:') }}</p>\n                <p><strong>{{ _(doc.in_words) }}</strong></p>\n            </td>\n            <td>{{ _('Total') }}</td>\n            <td><strong>{{ doc.get_formatted('total') }}</strong></td>\n        </tr>\n        {% if doc.discount_amount > 0 %}\n            <tr>\n                <td>{{ _('Discount') }}</td>\n                <td><strong>{{ doc.get_formatted('discount_amount') }}</strong></td>\n            </tr>\n        {% endif %}\n        <tr>\n            <td>{{ _('Grand Total') }}</td>\n            <td><strong>{{ doc.get_formatted('grand_total') }}</strong></td>\n        </tr>\n    </tfoot>\n</table>\n\n{% if doc.terms and (doc.terms|striptags|trim): %}\n    <div>\n        <h4>{{ _('Terms and Conditions') }}</h4>\n        <div style=\"font-size: 0.875em; color: #333;\">{{ doc.terms }}</div>\n    </div>\n{% endif %}\n\n{% if footer %}\n    {{ footer }}\n{% endif %}",
